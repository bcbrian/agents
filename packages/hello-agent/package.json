{
  "name": "@brianbuildz/hello-agent",
  "version": "0.1.2",
  "description": "A simple hello world agent following the Model Context Protocol (MCP)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "bin": {
    "hello-agent": "dist/index.js"
  },
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf dist",
    "dev": "tsc --watch",
    "lint": "eslint src --ext .ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "start": "node dist/index.js",
    "prepublishOnly": "npm run build",
    "publish:public": "npm publish --access public"
  },
  "keywords": [
    "hello-world",
    "mcp",
    "model-context-protocol",
    "agents"
  ],
  "author": "Brian Holt <brianbuildz@example.com>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.5.1",
    "eslint": "^8.47.0",
    "execa": "^9.5.2",
    "rimraf": "^5.0.1",
    "typescript": "^5.1.6",
    "vitest": "^3.0.8",
    "wait-on": "^8.0.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brianbuildz/agents.git"
  },
  "bugs": {
    "url": "https://github.com/brianbuildz/agents/issues"
  },
  "homepage": "https://github.com/brianbuildz/agents#readme"
}
